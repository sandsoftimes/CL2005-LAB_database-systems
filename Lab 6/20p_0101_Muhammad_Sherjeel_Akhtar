EX 4.9

SELECT PARK_COUNTRY, SUM(LINE_PRICE) AS TOTAL_SALES FROM THEMEPARK INNER JOIN SALES INNER JOIN SALES_LINE ON THEMEPARK.PARK_CODE=SALES.PARK_CODE AND SALES.TRANSACTION_NO=SALES_LINE.TRANSACTION_NO  WHERE PARK_COUNTRY='UK' OR PARK_COUNTRY='FR' GROUP BY PARK_COUNTRY
    -> ;
+--------------+-------------+
| PARK_COUNTRY | TOTAL_SALES |
+--------------+-------------+
| FR           |      401.86 |
| UK           |      851.00 |
+--------------+-------------+
2 rows in set (0.001 sec)

